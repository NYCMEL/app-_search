#!/Melify/bin/mac/tclkit

set items {
    accordion
    app
    band
    breadcrumb
    calendar
    card
    carousel
    center
    chart
    check
    dad
    dropdown
    dropper
    flip
    form
    groupbox
    icon
    listbox
    maker
    melify
    modal
    orgchart
    page
    popup
    radio
    select
    slide
    stocks
    swipe
    table-json
    table-plain
    table
    text
    textarea
    ticker
    tile
    toolbar
    typeahead
    wizard
}

set x "http://tk-search.melify.com/tk/lib/components/misc/webpack/prod"

set t [clock seconds]

set qstr "delete from search where ip='0.0.0.0'\;"
set tmp "INSERT OR REPLACE INTO search (id,url,description,tag,ip,ts) VALUES "

foreach i $items {
    append qstr "$tmp (null, '$x/$i', 'W3C Component - $i (production)', 'W3C Component prod $i', '0.0.0.0', '$t')\;"
}

set items {
    accordion
    app
    band
    card
    carousel
    center
    chart
    dad
    dropper
    flip
    form
    listbox
    maker
    matrix
    melify
    modal
    notify
    orgchart
    page
    popup
    slide
    stocks
    swipe
    table
    ticker
    tile
    toolbar
    typeahead
    wizard
}

set x "http://tk-search.melify.com/tk/lib/components/w/html"

set t [clock seconds]

set tmp "INSERT OR REPLACE INTO search (id,url,description,tag,ip,ts) VALUES "

foreach i $items {
    append qstr "$tmp (null, '$x/$i.html', 'W3C Component - $i (development)', 'W3C Component dev development $i', '0.0.0.0', '$t')\;"
}

set items {
    Cloud-Widgets.mp4
    TurboPraise.mp4
    HUB-IPR-iRE.mp4
    Jake.mp4
    Mia.mp4
    MelAndFamily.mp4
    Rapid-Prototyping.mp4
    FannieMae/fannie.mp4
    All-Hands-II.mp4
    Camtasia/MPL/MPL.mp4
    PinDrop.mp4
    Archives/Hub-IRE-I.mp4
    Archives/Responsive.mp4
    Archives/HUB-IRE.mp4
    Archives/All-Hands-III.mp4
    Archives/All-Hands.mp4
    Archives/Hub-Option-II.mp4
    Archives/HUB-II.mp4
    Archives/Hub-old.mp4
    Archives/IRE-V-II.mp4
    Archives/Remote-Editing.mp4
    Archives/IRE-V-I.mp4
    Archives/Soul.mp4
    Archives/Hub.mp4
    Hub-Goal-Creation.mp4
    TheCompany.mp4
    CodeGen/Generator.mp4
    BidRX.mp4
    Citi-RTDMS.mp4
    Demo.mp4
    New-Website.mp4
    Barclays/Barclays.mp4
    iPR-large.mp4
    PageBuilder.mp4
}

set x "http://tk-search.turnongreen.com"

set t [clock seconds]

set tmp "INSERT OR REPLACE INTO search (id,url,description,tag,ip,ts) VALUES "

foreach i $items {
    regsub -all {\-} $i " " j
    append qstr "$tmp (null, '$x/videos/$i', 'Video $i', 'video $j', '0.0.0.0', '$t')\;"
}

puts $qstr

